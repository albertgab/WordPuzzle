<Window x:Class="WordPuzzleWPF.Leaderboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordPuzzleWPF"
        mc:Ignorable="d"
        Background="#f8ede3"
        Title="Leaderboard" Height="420" Width="300">
    <Grid>
        <ListBox x:Name="listBox" Background="#bdd2b6" Margin="0,27,0,0"/>
        <TextBlock x:Name="textBlock" Text="Score Username  Time      Date" TextWrapping="Wrap" Margin="10,6,0,0" Height="21" VerticalAlignment="Top" HorizontalAlignment="Left" Width="290"/>

    </Grid>
</Window>
