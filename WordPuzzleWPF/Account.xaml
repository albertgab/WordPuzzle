<Window x:Class="WordPuzzleWPF.Account"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordPuzzleWPF"
        mc:Ignorable="d"
        Title="Account" Height="326" Width="333">
    <Grid>
        <Button x:Name="buttonBack" Content="Back to levels" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="buttonBack_Click"/>
        <TextBlock x:Name="textBlockUsername" HorizontalAlignment="Left" Margin="220,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="86" TextAlignment="Right"><Run Language="pl-pl" Text="Username"/></TextBlock>
        <TextBlock x:Name="textBlockScore" HorizontalAlignment="Left" Margin="111,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" TextAlignment="Center"><Run Language="pl-pl" Text="Score"/></TextBlock>
        <Grid Margin="0,35,0,0">
            <ListBox x:Name="listBox" Background="#bdd2b6" Margin="0,27,0,0"/>
            <TextBlock x:Name="textBlock" Text=" Level   Score       Time                  Date" TextWrapping="Wrap" Margin="0,6,0,0" Height="21" VerticalAlignment="Top" HorizontalAlignment="Center" Width="313"/>
        </Grid>
    </Grid>
</Window>
